package Day01;

public class factorial {

	private static void f1(int n) {
		int re =0;
		for(int i=1;i<=n;i++) {
			re+=factorial(i);
		}
	}
	private static int factorial(int n) {
		int re = 1; 
		for(int i=1;i<=n;i++) {
			re*=i;
		}
		return re;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
	}

}
