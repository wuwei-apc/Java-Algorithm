package Sort;

/**
 * 冒泡排序
 * @author zpc20
 *
 */
public class BubbleSort {
     
	public static void BubbleSort(int[] arr) {
	    // 边界
		if(arr.length<2||arr==null) {
			return;
		}
		int N = arr.length;
		for(int i=0;i<N;i++) 
		{
		    for(int j=i+1;i<N;j++) {
		    	
		    }
		}
	}
	
	
	public static void main(String[] args) {
	   	
	}
}
