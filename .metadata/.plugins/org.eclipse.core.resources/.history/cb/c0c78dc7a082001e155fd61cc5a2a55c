package Sort;

/**
 * 冒泡排序
 * @author zpc20
 *
 */
public class BubbleSort {
     
	public static void BubbleSort(int[] arr) {
	    // 边界
		if(arr.length<2||arr==null) {
			return;
		}
		int N = arr.length;
		for(int i=0;i<N;i++) 
		{
		    for(int j=i;j<N-i;j++) {
		    	swap()
		    }
		}
	}
	public static void swap(int[] arr,int j,int i) {
		 int tmp = arr[j];
		 arr[j] =arr[i];
		 arr[i] = tmp;
	}
	
	public static void main(String[] args) {
	   	
	}
}
