package Base;

public class Random1_5_0_7 {
    // 包函数不可变
	public static int f() {
		// 1-5等概率随机
		return (int)(Math.random()*5)+1;
	}
	public static int zeroP() {
		int ans=0;
		do {
			ans = f();
		}while(ans==3);
		// 1,2->0 3 45->1
		return ans<3?0:1;
	}
	public static void main(String[] args) {
		System.out.println("");
	}
}
