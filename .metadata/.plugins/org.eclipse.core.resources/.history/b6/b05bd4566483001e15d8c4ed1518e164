package Base;

public class Random1_5_0_7 {
    // 包函数不可变
	public static int f() {
		// 1-5等概率随机
		return (int)(Math.random()*5)+1;
	}
	public static int zeroP() {
		int ans;
		do {
			ans = f();
		}while(ans!=3);
		return ans;
	}
	public static void main(String[] args) {
		System.out.println("");
	}
}
